{
    "name": "@iwharris/aoc",
    "version": "0.0.0",
    "description": "Advent of Code solutions",
    "main": "dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "bin": {
        "aoc": "./dist/src/index.js"
    },
    "scripts": {
        "aoc": "node dist/src/index.js",
        "dev": "ts-node src/index.ts",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prettier": "prettier --check {src,test}/**/*.[jt]s",
        "prettier:fix": "prettier --write {src,test}/**/*.[jt]s",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run -s prettier"
        }
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/iwharris/aoc.git"
    },
    "keywords": [
        "adventofcode",
        "advent",
        "typescript"
    ],
    "author": "Ian Harris",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/iwharris/aoc/issues"
    },
    "homepage": "https://github.com/iwharris/aoc#readme",
    "devDependencies": {
        "@commander-js/extra-typings": "^9.4.1",
        "@iwharris/eslint-config": "^0.1.1",
        "@iwharris/prettier-config": "^0.0.4",
        "@types/glob": "^8.0.0",
        "@types/jest": "^29.2.3",
        "@types/node": "^18.11.10",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "jest": "^29.3.1",
        "prettier": "^1.19.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "commander": "^9.4.1",
        "glob": "^8.0.3"
    }
}

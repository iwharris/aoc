{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "plugin:prettier"],
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "warn",

        "no-unused-vars": ["error", { "args": "error" }]
    }
}
